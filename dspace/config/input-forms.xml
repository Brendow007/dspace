          <dc-qualifier>discipline</dc-qualifier>
          <repeatable>false</repeatable>
          <label>Disciplina</label>
          <input-type>onebox</input-type>
          <hint>Entre com o nome da disciplina para o qual o material foi criado.</hint>
          <required></required>
        </field>

      </page>

